<!-- compose -->
<%- include('partials/header') -%>
<h1>Compose</h1>

<!-- form -->
<form action="/compose" method="post">
    <div class="form-group">
        <label>Title</label>
        <input class="form-control" type="text" name="newTitle">
        <label>Post</label>
        <textarea class="form-control" name="postTitle" rows="5" cols="30"></textarea>
    </div>
    <button class="btn btn-light" type="submit" name="button">Publish</button>
</form>


<%- include('partials/footer') -%>