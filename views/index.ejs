<!-- content -->
<%- include('partials/header') -%>
<h1>Home</h1>
<p><%= title %> </p>

<% posts.forEach(element => { %>
    <h1> <%= element.title %> </h1>
    <p>
        <%= element.content.substring(0, 150) + "..." %>
        <a href="/posts/<%= element.title    %>">Read More</a>
    </p>
<% }) %>

<%- include('partials/footer') -%>